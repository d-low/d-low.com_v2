 <ul class="list-unstyled sub-content-items">
  <% for (var subcontents = content.get("subcontents"), i = subcontents.length - 1; i >= 0; i--) { 
      var randomImage = subcontents.at(i).getRandomPost().getRandomImage();
  %>
    <li class="sub-content-item content-item-container">
      <a class="sub-content-link" href="#content/<%= subcontents.at(i).get('path') %>">
        <figure class="sub-content-container">
          <span 
            class="sub-content-image js-sub-content-image" 
            data-backgroundimage="<%= !_.isNull(randomImage) ? randomImage.get('fullpath') : '' %>">
          </span>
          <figcaption class="sub-content-title">
            <%= subcontents.at(i).get("title") %>
          </figcaption>
        </figure>
      </a>
    </li>
  <% } %>
</ul>
